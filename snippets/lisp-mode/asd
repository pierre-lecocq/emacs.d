(asdf:defsystem #:${1:name}
    :author "`(user-full-name)` <`(user-mail-address)`>"
    :description "${2:description}"
    :license "MIT"
    :serial t
    :depends-on ($0)
    :components ((:file "package")
                 (:file "$1")))
